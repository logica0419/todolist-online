{
  "root": true,
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "no-console": "warn",
    "no-debugger": "warn",
    "vue/custom-event-name-casing": ["error", "camelCase"],
    "vue/attributes-order": ["warn", {
      "alphabetical": true
    }]
  }
}